export var camera_speed = 15
export var start_pos = -100
var enable_scene_change = false

func _ready():
	# Called every time the node is added to the scene.
	# Initialization here
	set_process_input(true)
	set_process(true)
	set_pos(Vector2(0, start_pos))
	pass

func _process(delta):
	var pos = get_pos()
	print(pos)
	set_pos(Vector2(pos.x, pos.y + camera_speed * delta))
	
	if(pos.y >= 180):
		enable_scene_change = true
	
	pass
	
func _input(event):
	if(enable_scene_change):
		get_node(\"/root/global\").goto_scene(\"res://scenes/Castle.tscn\")
	pass"

